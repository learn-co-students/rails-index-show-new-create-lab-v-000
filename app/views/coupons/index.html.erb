<div>
  <% @coupons.each do |coupon| %>
    <div><%=coupon.store%>
    <%= link_to coupon.coupon_code, coupon_path(coupon) %></div>
  <% end %>
</div>

<!-- The first argument to link_to is what the link will display, the second argument is the href attribute -->
<!-- coupon code is the name of the coupon-->