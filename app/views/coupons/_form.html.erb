<h3>Coupon Form</h3>
<%= form_tag coupons_path do %>
  <label>Coupon Code:</label>
  <%= text_field_tag :coupon_code %>
  <label>Store:</label>
  <%= text_field_tag :store %><br>
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <%= submit_tag "Submit Coupon" %>
<% end %>
