<h1>Coupon Form</h1>

<%= form_tag coupons_path(@coupon) do %>
  <%= label_tag "Coupon Code" %>
  <%= text_field_tag :'coupon[:coupon_code]'%>

  <%= label_tag "Store" %>
  <%= text_field_tag :'coupon[:store]'%>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

  <%= submit_tag "Submit Coupon"%>
<% end %>
