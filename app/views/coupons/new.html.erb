<h1>Coupon Form</h1>
<%= form_tag coupons_path do %>
  <label>Store: </label>
  <%= text_field_tag :'store' %><br>
  <label>Code: </label>
  <%= text_field_tag :'coupon_code' %><br>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <input type="submit" value="Submit Coupon">
<% end %>
