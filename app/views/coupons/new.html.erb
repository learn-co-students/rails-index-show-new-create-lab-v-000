<h3>Coupon Form</h3>
<%= form_tag coupons_path do %>
  <label>Coupon code:</label>
  <%= text_field_tag :coupon_code %><br></br>


  <label>Store:</label>
  <%= text_field_tag :store %><br></br>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= submit_tag :'Submit coupon' %>
<% end %>
