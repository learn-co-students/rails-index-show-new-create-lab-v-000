<h1>Coupon Form</h1>

<%= form_tag coupons_path do %>
  <%= label_tag "coupon_code" %>
  <%= text_field_tag "coupon_code" %><br><br>
  <%= label_tag "store" %>
  <%= text_field_tag "store" %><br><br>
  <%= hidden_field_tag :authenticity_token, :form_authenticity_token %>
  <%= submit_tag "Submit Coupon" %>
<% end %>
