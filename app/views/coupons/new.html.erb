
<h1> Coupon Form </h1>

<%= form_tag coupons_path do %>
  <%= label_tag "Coupon Code:" %>
  <br>
  <%= text_field_tag :"coupon[coupon_code]" %>
  <br>
  
  <%= label_tag "Store:" %>
  <br>
  <%= text_field_tag :"coupon[store]" %>
  <br>
  
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  
  <%= submit_tag "Submit Coupon" %>
<% end %>



