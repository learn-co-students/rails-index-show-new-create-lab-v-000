<h2>Coupon Form:</h2>

<%= form_tag coupons_path do %>
  <label for="coupon_code">Coupon Code: </label><br>
  <%= text_field_tag :'coupon[coupon_code]' %><br>

  <label for="store">Store: </label><br>
  <%= text_field_tag :'coupon[store]' %><br>

  <input type="submit" value="Submit Coupon"><br>
<% end %>
