<h3>Coupon Form</h3>

<%= form_tag coupons_path do %>

    <label>Coupon Code:</label><br>
    <input type="text" id="coupon_code" name="coupon[coupon_code]"><br>
    
   <label>Store:</label><br>
    <input type="text" id="store" name="coupon[store]"><br>
    
    <%= submit_tag "Submit Coupon" %>

<% end %>