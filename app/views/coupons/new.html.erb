<h1>Coupon Form</h1>

<%= form_tag(coupons_path, {method: "post"}) do%>


<%= label_tag(:coupon_code, "Coupon Code") %>
<%= text_field_tag(:'coupon[coupon_code]', params[:coupon], :id => "coupon_code") %>

<%= label_tag(:store, "Store") %>
<%= text_field_tag(:'coupon[store]', params[:coupon], :id => "store") %>

<%= submit_tag("Submit Coupon", :name => :submit, :id => :submit) %>
<% end %>